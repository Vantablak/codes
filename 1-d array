#include<stdio.h>
int main()
{
    int i,a[50],n;
    printf("Enter a number\n");
    scanf("%d",&n);
    printf("Enter %d number(s)\n",n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Output is \n");
    for(i=0;i<n;i++)
    {
        printf("a[%d]=%d\n",i,a[i]);
    }
    return 0;
}
